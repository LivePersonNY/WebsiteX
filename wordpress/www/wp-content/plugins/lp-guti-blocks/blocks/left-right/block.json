{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "lp-guti-blocks/left-right",
	"version": "0.1.0",
	"title": "Text + Image",
	"category": "liveperson",
	"icon": "align-pull-right",
	"description": "Simple module with text and image side by side.",
	"supports": {
		"html": false
	},
	"textdomain": "lp-guti-blocks",
	"editorScript": "file:../../build/all.js",
	"editorStyle": "file:../../build/all.css",
	"style": "file:../../build/style-all.css",
	"attributes": {
		"backgroundColor": {
			"default": "bg-transparent"
		},
		"title": {
			"default": "Valuable Text"
		},
		"text": {
			"default": "This image is supposed to be of value."
		},
		"mediaId": {
			"default": 0,
			"type": "number"
		},
		"mediaUrl": {
			"type": "string",
			"default": "https://picsum.photos/752/568?random=1"
		},
		"mediaAlt": {
			"type": "string",
			"default": ""
		},
		"flipped": {
			"type": "boolean",
			"default": false
		},
		"repeat": {
			"type": "boolean",
			"default": false
		},
		"linkText": {"default": "Here's a link text"},
		"linkUrl": {"default": "https://www.liveperson.com"}
	}
}
