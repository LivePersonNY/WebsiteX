{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "lp-guti-blocks/media-lottie-player",
	"version": "0.1.0",
	"title": "Lottie Player",
	"category": "media",
	"icon": "controls-repeat",
	"description": "Simple module with text and image side by side.",
	"supports": {
		"html": false
	},
	"textdomain": "lp-guti-blocks",
	"editorScript": "file:../../build/all.js",
	"editorStyle": "file:../../build/all.css",
	"style": "file:../../build/style-all.css",
	"attributes": {
		"lottieFile": {},
		"lottieId": {},
		"autoApproveLang": {"default":false, "type": "boolean"},
		"anchor": {},
		"alignment": {
			"default": "align-center"
		},
		"widthSetting": {
			"default": "w-100"
		},
		"mediaId": {
			"default": 0,
			"type": "number"
		},
		"mediaUrl": {
			"default": "https://picsum.photos/752/568?random=1"
		},
		"mediaAlt": {
			"type": "string",
			"default": ""
		}
	}
}
