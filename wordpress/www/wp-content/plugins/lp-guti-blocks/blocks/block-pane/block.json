{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "lp-guti-blocks/block-pane",
	"version": "0.1.0",
	"title": "Block Pane",
	"category": "liveperson",
	"icon": "block-default",
	"description": "Add a liveperson page block that supports WP blocks.",
	"supports": {
		"html": false
	},
	"textdomain": "lp-guti-blocks",
	"editorScript": "file:../../build/all.js",
	"editorStyle": "file:../../build/all.css",
	"style": "file:../../build/style-all.css",
	"attributes": {
		"gated": {
			"type": "boolean",
			"default": false
		},
		"backgroundColor": {"default": "bg-transparent"},
		"autoApproveLang": {"default":false, "type": "boolean"}
	}
}
